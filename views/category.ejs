<!Doctype html>
<html>
	<head>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>Oil Paintings | Artist Aditi</title>
		<link rel="stylesheet" href="/stylesheets/style.css" />
		<link rel="stylesheet" href="/stylesheets/jquery.lightbox.min.css" />

		<script type="text/javascript" src="/javascripts/jquery-1.11.2.min.js"></script>
		<script type="text/javascript" src="/javascripts/jquery.lightbox.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/masonry/3.3.1/masonry.pkgd.min.js"></script>
		<script type="text/javascript" src="/javascripts/script.js"></script>
	</head>
	<body>
		<div class="main-container">
			<% include ./partials/header %>
			<div class="content-area category">
				<h2 class="heading"><%= data.category.name %></h2>
				<div class="gallery">
					<% for(var i=0; i< data.imageSet.length; i++) { %>
						<div class="gallery-item with-border">
							<a href="<%= data.imageSet[i].url %>" rel="lightbox"><img src="<%= data.imageSet[i].url %>" /></a>
						</div>
					<% } %>
				</div>
			</div>
		<footer>
			Copyright &copy; Artist Aditi
		</footer>
	</body>
</html>