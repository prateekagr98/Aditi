<!Doctype html>
<html>
	<head>
		<title>Aditi Mittal</title>
		<link rel="stylesheet" href="/stylesheets/style.css" />

		<script type="text/javascript" src="/javascripts/jquery-1.11.2.min.js"></script>
		<script type="text/javascript" src="/javascripts/script.js"></script>
	</head>
	<body>
		<div class="main-container">
			<div class="sidebar">
				<div class="logo"></div>
				<p class="caption">An Artist</p>
				<div class="sidebar-items">	
					<div class="sidebar-item pink">Photography</div>
					<div class="sidebar-item dark-orange">Illustrations</div>
					<div class="sidebar-item purple">Unposted Letters</div>
					<div class="sidebar-item green">Blog</div>
					<div class="sidebar-item orange">About</div>
					<div class="sidebar-item blue">Contact</div>
				</div>
			</div>
			<div class="content-area">
				<div class="homepage-slider">
					<% for(var i=0; i<sliderSet.length; i++){ %>
					<div class="homepage-slider-item" style="background-image: url('<%= sliderSet[i] %>')"></div>
					<% } %>
				</div>
			</div>
			<div class="clear"></div>
		</div>
	</body>
</html>