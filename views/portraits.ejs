<!Doctype html>
<html>
	<head>
		<title>Portraits | Artist Aditi</title>
		<link rel="stylesheet" href="/stylesheets/style.css" />
		<link rel="stylesheet" href="/stylesheets/jquery.lightbox.min.css" />

		<script type="text/javascript" src="/javascripts/jquery-1.11.2.min.js"></script>
		<script type="text/javascript" src="/javascripts/jquery.lightbox.min.js"></script>
		<script type="text/javascript" src="/javascripts/script.js"></script>
	</head>
	<body>
		<div class="main-container">
			<% include ./partials/sidebar %>
			<div class="content-area">
				<div class="portraits">
					<% for(var i=0; i< imageSet.length; i++) { %>
						<div class="portraits-item">
							<a href="<%= imageSet[i] %>" rel="lightbox">
								<img src="<%= imageSet[i] %>" />
							</a>
						</div>
					<% } %>
				</div>
			</div>
			<div class="clear"></div>
		</div>
	</body>
</html>